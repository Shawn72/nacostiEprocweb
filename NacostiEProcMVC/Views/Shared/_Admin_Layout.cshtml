<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>NACOSTI - E-Procurement Portal</title>
    <link rel="icon" href="/images/logo.png" type="image/x-icon">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,400,500,600,700" rel="stylesheet">
    <link rel="stylesheet"   type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.2/css/all.min.css"/>
    <link rel="stylesheet" type="text/css" href="/assets/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/line-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="/assets/plugins/morris/morris.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/dataTables.bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/select2.min.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/bootstrap-datetimepicker.min.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/style.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/customprofile.css"/>
    <link rel="stylesheet" type="text/css" href="/assets/css/customstylesheet.css"/>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.8.0/css/bootstrap-datepicker3.min.css" rel="stylesheet" />
    <link href="http://ajax.aspnetcdn.com/ajax/jquery.ui/1.8.9/themes/blitzer/jquery-ui.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="/assets/css/bootstrap-select.min.css" />
    <link rel="stylesheet" href="~/Content/sweetalert/sweet-alert.css"/>


</head>
<body>
<div class="main-wrapper">
 
    <div class="header">
        <div class="header-left">
            <a href="/Home/Index" class="logo">
                <img src="/assets/img/logo.png" width="85" height="40" alt="">
            </a>
        </div>
        <a id="toggle_btn" href="javascript:void(0);"><i class="la la-bars"></i></a>
        <div class="page-title-box pull-left">
            <h3>NACOSTI</h3>
        </div>
        <a id="mobile_btn" class="mobile_btn pull-left" href="#sidebar"><i class="fa fa-bars" aria-hidden="true"></i></a>
        <ul class="nav navbar-nav navbar-right user-menu pull-right">
            <li class="dropdown">
                <a href="Profile.aspx" class="dropdown-toggle user-link" data-toggle="dropdown" title="Admin">
                    <span class="user-img"><img class="img-circle" src="/assets/img/avatar.png" width="40" alt="Admin">
                        <span class="status online"></span></span>
                    <span>@Session["name"]</span>
                    <i class="caret"></i>
                </a>
                <ul class="dropdown-menu">
                    <li><a href="/Home/Profile">Profile</a></li>
                    <li><a href="/Home/ChangePassword">Change Password</a></li>
                    <li><a href="/Home/CheckLogout">Logout</a></li>
                </ul>
            </li>
        </ul>
        <div class="dropdown mobile-user-menu pull-right">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><i class="fa fa-ellipsis-v"></i></a>
            <ul class="dropdown-menu pull-right">
                <li><a href="/Home/ChangePassword">Change Password</a></li>
                <li><a href="/Home/CheckLogout">Logout</a></li>
            </ul>
        </div>
    </div>
        
    <div class="sidebar" id="sidebar">
        <div class="sidebar-inner slimscroll">
            <div id="sidebar-menu" class="sidebar-menu">
                <ul>
                    <li class="active"><a href="/Home/AdminIndex"><i class="glyphicon glyphicon-home"></i> <span>Dashboard</span></a></li>
                    <li><a href="/Home/AllContactsList"><i class="glyphicon glyphicon-user"></i> <span>All Customers</span></a></li>
                    <li class="submenu">
                        <a href="#"><i class="fas fa-folder-open fa-fw"></i> <span> RFQ Applications</span> <span class="menu-arrow"></span></a>
                        <ul style="display: none;">
                            <li><a href="#"><span>RFQ Applications</span></a></li>
                            <li><a href="#"><span>Tender Applications</span></a></li>
                        </ul>
                    </li>
                 
                    <li class="submenu">
                        <a href="#"><i class="fas fa-folder-open fa-fw"></i> <span> Tender Applications</span> <span class="menu-arrow"></span></a>
                        <ul style="display: none;">
                            <li><a href="/Home/AllTenderApplications"><span>Applied Tenders</span> </a></li>
                            <li><a href="#"><span>Awarded Tenders</span> </a></li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
    </div>  


    <div class="page-wrapper">
        @RenderBody()
        <footer class="main-footer ffooter">
            <strong>Copyright &copy; @DateTime.Now.Year  - <a href="https://dynasoft.co.ke" target="_blank">Dynasoft Business Solutions Limited</a>.</strong> All rights
            reserved.
        </footer>
    </div>

</div>
    
<div class="sidebar-overlay" data-reff="#sidebar"></div>
<script type="text/javascript" src="/assets/js/jquery-3.2.1.min.js"></script>
<script type="text/javascript" src="/assets/js/bootstrap.min.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>  
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
<script type="text/javascript" src="/assets/js/jquery.slimscroll.js"></script>
<script type="text/javascript" src="/assets/plugins/morris/morris.min.js"></script>
<script type="text/javascript" src="/assets/plugins/raphael/raphael-min.js"></script>
<script type="text/javascript" src="/assets/js/jquery.dataTables.min.js"></script>
<script type="text/javascript" src="/assets/js/dataTables.bootstrap.min.js"></script>
<script type="text/javascript" src="/assets/js/jquery.slimscroll.js"></script>
<script type="text/javascript" src="/assets/js/select2.min.js"></script>
<script type="text/javascript" src="/assets/js/moment.min.js"></script>
<script type="text/javascript" src="/assets/js/bootstrap-datetimepicker.min.js"></script>
<script type="text/javascript" src="/assets/js/app.js"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@8"></script>
<script type="text/javascript" src="/assets/js/bootstrap-select.min.js"></script>

@*custom javascripts*@
<script src="~/assets/customassets/buttonClicks.js" type="text/javascript"></script>
<script src="/assets/customassets/myCustomscripts.js" type="text/javascript" ></script>
<script src="/assets/customassets/jsFunctions.js" type="text/javascript" ></script>
<script src="/assets/js/countrypicker.js" type="text/javascript" ></script>

</body>
</html>
